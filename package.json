{
  "name": "afcog",
  "version": "1.0.0",
  "description": "afcog",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.freelance.afco",
    "dmg": {
      "contents": [
        {
          "x": 150,
          "y": 180
        },
        {
          "x": 380,
          "y": 180,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freelance-work/mit_psycology_dept.git"
  },
  "author": "freelance-work",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freelance-work/mit_psycology_dept/issues"
  },
  "homepage": "https://github.com/freelance-work/mit_psycology_dept#readme",
  "dependencies": {
    "async-mkdirp": "^1.2.7",
    "electron-store": "^2.0.0",
    "json2csv": "^4.3.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^20.38.2",
    "fs": "0.0.1-security"
  }
}
